    "version": "0.2.0",
    "configurations": [
        {
            "preLaunchTask": "run make",
            "name": "Launch Task 1 cube",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/bin/bin/task1",
            "args": [
                "-t 4",
                "data/task1/cube.json"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
